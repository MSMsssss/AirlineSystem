<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="msm.view.LoginSceneController">
   <children>
      <ImageView fitHeight="54.0" fitWidth="56.0" layoutX="400.0" layoutY="196.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../sourcematerial/picture/user.png" />
         </image>
      </ImageView>
      <TextField fx:id="userEdit" layoutX="301.0" layoutY="300.0" prefHeight="36.0" prefWidth="251.0" promptText="输入用户名" />
      <Label layoutX="223.0" layoutY="306.0" text="用户名" />
      <Label layoutX="232.0" layoutY="369.0" text="密码" />
      <PasswordField fx:id="passwordEdit" layoutX="301.0" layoutY="363.0" prefHeight="36.0" prefWidth="251.0" promptText="输入密码" />
      <Button layoutX="327.0" layoutY="432.0" mnemonicParsing="false" onAction="#clickLogin" prefHeight="36.0" prefWidth="73.0" text="登录" />
      <Button layoutX="454.0" layoutY="432.0" mnemonicParsing="false" onAction="#clickRegister" prefHeight="36.0" prefWidth="73.0" text="注册" />
      <Label fx:id="loginMessage" layoutX="355.0" layoutY="267.0" prefHeight="24.0" prefWidth="144.0" textFill="#e40e0e" />
      <ImageView fitHeight="36.0" fitWidth="36.0" layoutX="14.0" layoutY="14.0" onMouseClicked="#clickBackButton" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../sourcematerial/picture/back.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
